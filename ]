#include <iostream>
#include "../Libs/Validator.h"

using namespace std;

int main(void) {
    Validator validator;
    BirthDate b = {
        ._day = 32,
        ._month = 1,
        ._year = 2005,
    };
    
    VALIDATOR_ERROR_TYPE error = validator.isBirthDateValid(b);

    switch (error) {
        case VALIDATOR_ERROR_TYPE::DAY_ERROR:
        case VALIDATOR_ERROR_TYPE::MONTH_ERROR:
        case VALIDATOR_ERROR_TYPE::YEAR_ERROR:
            cout << "There're some errors\n";
            break;

        case VALIDATOR_ERROR_TYPE::NEGATIVE_NUMBER_ERROR:
            cout << "WTF\n";

        case VALIDATOR_ERROR_TYPE::NO_ERROR:
            cout << "PASSED\n";
    
    }


    return 0;
}

